{
  "name": "sports-venues-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start": "node dist/main.js",
    "build": "tsc -p tsconfig.build.json",
    "seed": "ts-node src/seed.ts",
    "add-venue": "ts-node src/add-venue.ts",
    "add-venue:interactive": "ts-node src/add-venue-interactive.ts",
    "add-venue:csv": "ts-node src/add-venue-csv.ts",
    "view-data": "ts-node src/view-data.ts",
    "set-admin": "ts-node src/set-admin.ts",
    "reset-password": "ts-node src/reset-password.ts",
    "migrate:page-view": "ts-node src/migrations/run-migration.ts",
    "migrate:alter-user-agent": "ts-node src/migrations/run-alter-migration.ts",
    "migrate:add-contact": "ts-node src/migrations/run-add-contact-migration.ts",
    "migrate:add-is-public": "ts-node src/migrations/run-add-is-public-migration.ts",
    "migrate:add-district-code": "ts-node src/migrations/run-add-district-code-migration.ts",
    "migrate:add-court-count": "ts-node src/migrations/run-add-court-count-migration.ts",
    "migrate:add-venue-facilities": "ts-node src/migrations/run-add-venue-facilities-migration.ts",
    "migrate:add-geom": "ts-node src/migrations/add-geom-column.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d src/data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate src/migrations/AutoMigration",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "git:push": "cd ../.. && ./git-push.sh",
    "git:push:auto": "cd ../.. && ./git-push.sh \"Update: $(date +'%Y-%m-%d %H:%M:%S')\""
  },
  "dependencies": {
    "@nestjs/common": "10.3.7",
    "@nestjs/core": "10.3.7",
    "@nestjs/platform-express": "10.3.7",
    "@nestjs/typeorm": "10.0.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "16.4.5",
    "reflect-metadata": "0.1.14",
    "rxjs": "7.8.1",
    "pg": "8.11.5",
    "typeorm": "0.3.20",
    "ali-oss": "6.18.1",
    "crypto": "^1.0.1",
    "sharp": "0.33.2",
    "multer": "1.4.5-lts.1",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/passport": "10.0.3",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "bcrypt": "5.1.1"
  },
  "devDependencies": {
    "@types/node": "20.14.9",
    "@types/multer": "1.4.11",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5",
    "@types/passport-jwt": "3.0.13",
    "@types/passport-local": "1.0.38",
    "@types/bcrypt": "5.0.2"
  }
}
